{
  "name": "sqlutil",
  "version": "3.0.0",
  "description": "Wrapper around sqlite for structured queries",
  "homepage": "https://github.com/erikman/sqlutil",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "babel --compact true --minified --source-maps false -d dist/ lib",
    "build:debug": "babel -d dist/ lib",
    "lint": "eslint lib test",
    "test": "npm run test:unittests && npm run test:snyk",
    "test:snyk": "snyk test",
    "test:unittests": " mocha --require babel-polyfill --compilers js:babel-core/register test",
    "prepare": "npm run snyk-protect; npm run build",
    "snyk-protect": "snyk protect"
  },
  "keywords": [
    "sqlite",
    "promise",
    "database",
    "sql"
  ],
  "author": {
    "name": "Erik Man",
    "email": "goodbyte100@yahoo.com"
  },
  "license": "MIT",
  "repository": "erikman/sqlutil",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^3.15.0",
    "eslint-config-xo-space": "^0.15.0",
    "mocha": "^3.2.0",
    "streamutil": "^1.1.1"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "lodash": "^4.17.10",
    "sqlite3": "^4.0.1",
    "snyk": "^1.88.0"
  },
  "snyk": true
}
